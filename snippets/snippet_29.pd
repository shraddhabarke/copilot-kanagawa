template <typename T, auto N /*< Maximum size.*/>
struct vector
{
    T[N] data;
    count_t<N> size;
}

template <typename T, auto N>
inline optional<T> first_valid(vector<optional<T>, N> v)
{
    return reduce(selecting(on1st(Opt::is_valid<T>)), v);
}
